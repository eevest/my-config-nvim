" [General Configs]
set number
set mouse=a
set numberwidth=1
set clipboard=unnamed
syntax on
"filetype plugin indent on
set ruler
set showcmd
set nocursorline
set encoding=utf-8
set showmatch
set sw=2
set autoindent
set relativenumber
set lazyredraw
set ttyfast
set regexpengine=2
set updatetime=300
set laststatus=2
set noshowmode
set pyxversion=3

" SearchFiles
set incsearch
set ignorecase
set smartcase
